{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "plugins": [
      {
        "name": "@astrojs/ts-plugin"
      }
    ],
    "paths": {
			"@/*": ["src/*"],
      "@components/*": [
        "src/components/*"
      ],
      "@layouts/*": [
        "src/layouts/*"
      ],
      "@utils/*": [
        "src/utils/*"
      ],
      "@assets/*": [
        "src/assets/*"
      ],
      "@plugins/*": [
        "src/plugins/*"
      ],
      "@serverPlugins/*": [
        "src/serverPlugins/*"
      ]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "solid-js","types": ["astro/client"]
  }
}