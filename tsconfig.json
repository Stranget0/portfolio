{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "plugins": [
      {
        "name": "@astrojs/ts-plugin"
      }
    ],
		// "types": [
    //   "cypress",
    //   "cypress-visual-regression"
		// ],
    "paths": {
			"@/*": ["src/*"],
      "@components/*": [
        "src/components/*"
      ],
      "@layouts/*": [
        "src/layouts/*"
      ],
      "@utils/*": [
        "src/utils/*"
      ],
      "@assets/*": [
        "src/assets/*"
      ],
      "@plugins/*": [
        "src/plugins/*"
      ],
      "@serverPlugins/*": [
        "src/serverPlugins/*"
      ]
    },
    "jsx": "preserve",
    "jsxImportSource": "solid-js"
  }
}